$mol_tabler_demo : $mol_tabler
	header : $mol_tabler_demo_header

$mol_tabler_demo_header : $mol_block child
	< cellId : $mol_number
		editable : false
		value : =ID
	< cellFlag : $mol_stringer
		editable : false
		value : =Active
	< cellNumber : $mol_number
		editable : false
		value : =Age
	< cellString : $mol_stringer
		editable : false
		value : =Title

$mol_tabler_demo_row : $mol_block child
	< cellId : $mol_number
		editable : false
		value < id : =
	< cellFlag : $mol_checker
	< cellNumber : $mol_number value : =12345
	< cellString : $mol_stringer value < stringContent : =Hello, World!
