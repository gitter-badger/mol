$mol_panel_demo : $mol_rower child
	< light : $mol_panel
		head : =head
		body : =body
		foot : =foot
	< generic : $mol_panel
		head : =head
		body < genericBody : $mol_filler
		foot : =foot
	< bloat : $mol_panel
		head < bloatHead : $mol_filler
		body < bloatBody : $mol_filler
		foot < bloatFoot : $mol_filler
	< footless : $mol_panel
		head : =head
		body < footleassBody : $mol_filler
		foot : null
